<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>

<VBox xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="GUI.Controller.OrderCardController" onMouseClicked="#handleClick"
      spacing="10" alignment="TOP_CENTER">
    <StackPane>
        <ImageView  fitHeight="100" fitWidth="100" preserveRatio="true" />
        <ImageView  fitHeight="20" fitWidth="20" StackPane.alignment="TOP_RIGHT" />
    </StackPane>
    <VBox>
        <Label fx:id="productName"  />
        <Label fx:id="productId" />
    </VBox>
</VBox>